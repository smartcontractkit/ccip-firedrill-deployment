  "events": [
    {
      "name": "CCIPMessageSent",
      "fields": [
        {
          "name": "destChainSelector",
          "type": "u64",
          "index": false
        },
        {
          "name": "sequenceNumber",
          "type": "u64",
          "index": false
        },
        {
          "name": "message",
          "type": {
            "defined": "SVM2AnyRampMessage"
          },
          "index": false
        }
      ]
